{
  "name": "applyneu",
  "version": "1.0.0",
  "description": "Electron app with React frontend",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:react": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "electron .",
    "electron": "electron ."
  },
  "keywords": [
    "electron",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/pg": "^8.16.0",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "express": "^5.2.1",
    "pg": "^8.16.3"
  }
}
